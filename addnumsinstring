#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <numeric>

using namespace std;

/**
 * Auto-generated code below aims at helping you parse
 * the standard input according to the problem statement.
 **/

int main()
{
 
string num;
cin >> num; cin.ignore();

int accum = std::accumulate(num.begin(), num.end(), 1, [](int acc, char c) {
	return acc * (c - '0');
	});
cout << accum;
}
